<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>rpm.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <emission>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>/engines/engine[2]/rpm</property>
    <interpolation>
      <entry><ind>    0 </ind><dep>   0 </dep></entry>
      <entry><ind>  100 </ind><dep>  33 </dep></entry>
      <entry><ind>  200 </ind><dep>  71 </dep></entry>
      <entry><ind>  300 </ind><dep> 109 </dep></entry>
      <entry><ind>  400 </ind><dep> 144 </dep></entry>
      <entry><ind>  500 </ind><dep> 180 </dep></entry>
      <entry><ind>  600 </ind><dep> 218 </dep></entry>
      <entry><ind>  700 </ind><dep> 252 </dep></entry>
      <entry><ind>  800 </ind><dep> 286 </dep></entry>
      <entry><ind>  900 </ind><dep> 319 </dep></entry>
      <entry><ind> 1200 </ind><dep> 459 </dep></entry>
      <entry><ind> 1300 </ind><dep> 487 </dep></entry>
      <entry><ind> 1400 </ind><dep> 510 </dep></entry>
      <entry><ind> 1500 </ind><dep> 540 </dep></entry>
      <entry><ind> 1600 </ind><dep> 570 </dep></entry>
      <entry><ind> 1700 </ind><dep> 605 </dep></entry>
      <entry><ind> 1800 </ind><dep> 635 </dep></entry>
      <entry><ind> 1900 </ind><dep> 662 </dep></entry>
      <entry><ind> 2000 </ind><dep> 697 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>
